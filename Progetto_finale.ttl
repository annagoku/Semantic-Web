###  http://www.semanticweb.org/annag/ontologies/2024/1/horseOntology#CavalloASangueCaldo
:CavalloASangueCaldo rdf:type owl:Class ;
                     owl:equivalentClass [ owl:intersectionOf ( :Cavallo
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty :haIndole ;
                                                                  owl:someValuesFrom :SangueCaldo
                                                                ]
                                                              ) ;
                                           rdf:type owl:Class
                                         ] ;
                     rdfs:subClassOf :Cavallo .


###  http://www.semanticweb.org/annag/ontologies/2024/1/horseOntology#CavalloASangueFreddo
:CavalloASangueFreddo rdf:type owl:Class ;
                      owl:equivalentClass [ rdf:type owl:Restriction ;
                                            owl:onProperty :haRazza ;
                                            owl:someValuesFrom :RazzeASangueFreddo
                                          ] ;
                      rdfs:subClassOf :Cavallo .


###  http://www.semanticweb.org/annag/ontologies/2024/1/horseOntology#CavalloAndaluso
:CavalloAndaluso rdf:type owl:Class ;
                 owl:equivalentClass [ rdf:type owl:Restriction ;
                                       owl:onProperty :haRazza ;
                                       owl:someValuesFrom :Andaluso
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :èUtilizzatoIn ;
                                       owl:someValuesFrom :AltaScuola
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :haMantello ;
                                       owl:hasValue :baio
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :originarioDi ;
                                       owl:hasValue :Andalusia
                                     ] ;
                 rdfs:subClassOf :CavalloASangueCaldo .


