<http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses> rdf:type owl:Ontology ;
                                                                 owl:imports <http://www.ontologydesignpatterns.org/cp/owl/list.owl> ,
                                                                             <http://www.ontologydesignpatterns.org/cp/owl/partof.owl> ,
                                                                             <http://www.ontologydesignpatterns.org/cp/owl/price.owl> ;
                                                                 dc:creator "Annalisa Sabatelli" ;
                                                                 dc:date "2024-03-01T09:00:00"^^xsd:dateTime ;
                                                                 dc:description "Ontological representation of breeds, uses and main characteristics of horses and their sales announcements aimed at helping the user in purchasing a horse."@en ,
                                                                                "Rappresentazione ontologica di razze, impieghi e caratteristiche principali dei cavalli e annunci di vendita finalizzata ad aiutare l'utente nell'acquisto di un cavallo."@it ;
                                                                 dc:title "OntoHorses"@en ;
                                                                 vann:preferedNamespacePrefix "http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses"@it ,
                                                                                              "oh"@en ;
                                                                 rdfs:label "Ontologia sulle principali razze di cavallo europee"@it ,
                                                                            "Ontology on the main European horse breeds"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
dc:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
dc:date rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
dc:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferedNamespacePrefix
vann:preferedNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
vann:preferredNamespaceUri rdf:type owl:AnnotationProperty .


###  http://schema.org/image
schema:image rdf:type owl:AnnotationProperty .


###  http://www.ontologydesignpatterns.org/cp/owl/bag.owl#hasItem
<http://www.ontologydesignpatterns.org/cp/owl/bag.owl#hasItem> rdf:type owl:AnnotationProperty .


###  http://www.ontologydesignpatterns.org/cp/owl/price.owl#hasCurrency
designpatternprice:hasCurrency rdf:type owl:AnnotationProperty .


###  http://www.ontologydesignpatterns.org/cp/owl/price.owl#hasPrice
designpatternprice:hasPrice rdf:type owl:AnnotationProperty ;
                            rdfs:domain oh:Cavallo .


###  http://www.ontologydesignpatterns.org/cp/owl/price.owl#hasValue
designpatternprice:hasValue rdf:type owl:AnnotationProperty .


###  http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#precedes
<http://www.ontologydesignpatterns.org/cp/owl/sequence.owl#precedes> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#exactMatch
skos:exactMatch rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#prefLabel
skos:prefLabel rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.ontologydesignpatterns.org/cp/owl/partof.owl#hasPart
designpatternpart:hasPart rdfs:domain oh:AreaGeografica ;
                          rdfs:range oh:AreaGeografica .


###  http://www.ontologydesignpatterns.org/cp/owl/partof.owl#isPartOf
designpatternpart:isPartOf rdfs:domain oh:AreaGeografica ;
                           rdfs:range oh:AreaGeografica .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses#adattoAlConseguimentoDi
oh:adattoAlConseguimentoDi rdf:type owl:ObjectProperty ;
                           rdfs:domain oh:Cavallo ;
                           rdfs:range oh:Patente ;
                           owl:propertyChainAxiom ( oh:gareggiaNelleCategorieDiSalto
                                                    oh:permetteIlConseguimentoDi
                                                  ) ;
                           rdfs:comment "Property that expresses the relationship between the horse's sporting performance and the riding licences it allows to achieve"@en ,
                                        "Proprietà che esprime la relazione tra le prestazioni sportive del cavallo e le autorizzazioni a montare che permette di conseguire"@it ;
                           rdfs:label "suitable for achieving"@en ,
                                      "adatto al conseguimento di" .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses#consegue
oh:consegue rdf:type owl:ObjectProperty ;
            owl:inverseOf oh:vieneConseguitoDa ;
            rdfs:domain oh:Cavaliere ;
            rdfs:range oh:Patente ;
            rdfs:comment "Property that expresses the relationship between the rider's experience and the riding licences he can achieve"@en ,
                         "Proprietà che esprime la relazione tra l'esperienza del cavaliere e l'<utorizzazioni a montare che può conseguire"@it ;
            rdfs:label "achieve"@en ,
                       "consegue"@it .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses#entitàWikidata
oh:entitàWikidata rdf:type owl:ObjectProperty ,
                           owl:FunctionalProperty ,
                           owl:InverseFunctionalProperty ;
                  rdfs:domain oh:RazzaCavallo ;
                  rdfs:range owl:Thing ;
                  rdfs:comment "La corrispondente entità su Wikidata"@it ,
                               "The corrispondig entity on Wikidata"@en ;
                  rdfs:label "Entità Wikidata"@it ,
                             "Wikidata Entity"@en .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses#gareggiaNellaCategoriaDiDressage
oh:gareggiaNellaCategoriaDiDressage rdf:type owl:ObjectProperty ;
                                    rdfs:domain dbo:horse ;
                                    rdfs:range oh:CategorieDressage ;
                                    rdfs:comment "Property that expresses the category of  dressage competition in which the horse usually participates."@en ,
                                                 "Proprietà che esprime la categoria di gara di dressage alla quale il cavallo di solito partecipa"@it ;
                                    rdfs:label "gareggia nella categoria di dressage"@it ,
                                               "take part to dressage category"@en .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses#gareggiaNelleCategorieDiSalto
oh:gareggiaNelleCategorieDiSalto rdf:type owl:ObjectProperty ;
                                 rdfs:domain oh:Cavallo ;
                                 rdfs:range oh:CategorieDiSalto ;
                                 rdfs:comment "Property that expresses the category of  jumping competition in which the horse usually participates."@en ,
                                              "Proprietà che esprime la categoria di gare di salto ad ostacoli alle quali il cavallo di solito partecipa."@it ;
                                 rdfs:label "gareggia nella categoria di salto ostacoli"@it ,
                                            "take part to jumping categories"@en .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses#haClassificazioneMorfologica
oh:haClassificazioneMorfologica rdf:type owl:ObjectProperty ;
                                rdfs:domain oh:RazzaCavallo ;
                                rdfs:range oh:RazzaCavallo ;
                                rdfs:comment "Property that expresses the morphological type to which the horse breed belongs"@en ,
                                             "Proprietà che esprime il tipo morfologico al quale la razza di cavallo appartiene"@it ;
                                rdfs:label "ha classificazione morfologica"@it ,
                                           "has morphological classification"@en .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses#haIndole
oh:haIndole rdf:type owl:ObjectProperty ,
                     owl:FunctionalProperty ;
            rdfs:domain oh:RazzaCavallo ;
            rdfs:range oh:Indole ;
            rdfs:comment "Property that expresses the temperament of a horse breed"@en ,
                         "Proprietà che esprime il temperamento di una razza di cavallo"@it ;
            rdfs:label "ha indole"@it ,
                       "has temperament"@en .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses#haNazionalità
oh:haNazionalità rdf:type owl:ObjectProperty ;
                 rdfs:domain oh:RazzaCavallo ;
                 rdfs:range oh:AreaGeografica ;
                 owl:propertyChainAxiom ( oh:haRegioneDiOrigine
                                          designpatternpart:isPartOf
                                        ) ;
                 rdfs:comment "Property that expresses the nation of origin of a horse breed"@en ,
                              "Proprietà che esprime la nazionalità di una razza di cavallo"@it ;
                 rdfs:label "has nationality"@en ,
                            "ha nazionalità" .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses#haRazza
oh:haRazza rdf:type owl:ObjectProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain oh:Cavallo ;
           rdfs:range oh:RazzaCavallo ;
           rdfs:comment "Property that expresses the breed of a horse"@en ,
                        "Proprietà che esprime la razza di un cavallo"@it ;
           rdfs:label "ha razza"@it ,
                      "has breed"@en .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses#haRegioneDiOrigine
oh:haRegioneDiOrigine rdf:type owl:ObjectProperty ;
                      rdfs:domain oh:RazzaCavallo ;
                      rdfs:range oh:AreaGeografica ;
                      rdfs:comment "Property that expresses the territory of origin of a horse breed"@en ,
                                   "Proprietà che esprime il territorio di origine di una razza di cavallo"@it ;
                      rdfs:label "ha Regione di origine"@it ,
                                 "has region of origin"@en .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses#permetteIlConseguimentoDi
oh:permetteIlConseguimentoDi rdf:type owl:ObjectProperty ;
                             rdfs:domain oh:CategorieDiSalto ;
                             rdfs:range oh:Patente ;
                             rdfs:comment "Property that expresses the relationship between the jumping categories in which a rider competes and the riding licences he can obtain. In particular, the C80 allows the obtaining of the patent; the C115 allows the achievement of the 1st degree and the C135 the 2nd degree,"@en ,
                                          "Proprietà che esprime la relazione tra le categorie di salto ad ostacoli in cui un cavaliera gareggia e le autorizzazioni a montare che può conseguire. In particolare la C80 permette il conseguimento del brevetto; la C115 permette il conseguimento del 1° grado e la C135 del 2° grado"@it ;
                             rdfs:label "allows the achievement of"@en ,
                                        "permette il conseguiimento di"@it .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses#possiede
oh:possiede rdf:type owl:ObjectProperty ;
            owl:inverseOf oh:èPossedutoDa ;
            rdfs:comment "Inverse proprerty of \"is owned by\""@en ,
                         "Proprietà inversa di \"è posseduto da\""@it ;
            rdfs:label "is owner"@en ,
                       "possiede"@it .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses#puòEssereImpiegataIn
oh:puòEssereImpiegataIn rdf:type owl:ObjectProperty ;
                        rdfs:domain oh:RazzaCavallo ;
                        rdfs:range oh:Impiego ;
                        rdfs:comment "Property that expresses the possible uses of a breed in work or sport"@en ,
                                     "Proprietà che esprime i possibili impieghi di una razza nel lavoro o nello sport"@it ;
                        rdfs:label "can be used in"@en ,
                                   "può essere impiegata in"@it .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses#vieneConseguitoDa
oh:vieneConseguitoDa rdf:type owl:ObjectProperty ;
                     rdfs:domain oh:Patente ;
                     rdfs:range oh:Cavaliere ;
                     rdfs:comment "Inverse property of \"achieve\""@en ,
                                  "Proprietà inversa di \"consegue\""@it ;
                     rdfs:label "is achieved by"@en ,
                                "viene conseguito da"@it .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses#vieneUtilizzatoPer
oh:vieneUtilizzatoPer rdf:type owl:ObjectProperty ;
                      rdfs:domain oh:Cavallo ;
                      rdfs:range oh:Impiego ;
                      rdfs:comment "Property that expresses the use made of a horse in work or sporting activities."@en ,
                                   "Proprietà che esprime l'utilizzo che viene fatto di un cavallo in attività lavorative o sportive."@it ;
                      rdfs:label "is used for"@en ,
                                 "viene utilizzato per"@it .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses#èAdattoPer
oh:èAdattoPer rdf:type owl:ObjectProperty ;
              rdfs:domain oh:Cavallo ;
              rdfs:range oh:Cavaliere ;
              owl:propertyChainAxiom ( oh:adattoAlConseguimentoDi
                                       oh:vieneConseguitoDa
                                     ) ;
              rdfs:comment "Property that expresses the suitability of a horse for the sporting abilities of a rider. Generally horses that are used in very high jumping categories are not suitable for inexperienced riders."@en ,
                           "Proprietà che esprime l'adeguatezza di un cavallo alle capacità sportive di un cavaliere. Generalmente cavalli che vengono utilizzati in categorie di salto molto alte non sono idonei a cavalieri poco esperti."@it ;
              rdfs:label "is suitable for"@en ,
                         "è adatto per"@it .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses#èPossedutoDa
oh:èPossedutoDa rdf:type owl:ObjectProperty ;
                rdfs:domain oh:Cavallo ;
                rdfs:range oh:Proprietario ;
                rdfs:comment "Property that expresses the relationship between the horse for sale and the current owner"@en ,
                             "Proprietà che esprime la relazione tra il cavallo in vendita e l'attuale proprietario"@it ;
                rdfs:label "is owned by"@en ,
                           "è posseduto da"@it .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses#èScuderizzatoIn
oh:èScuderizzatoIn rdf:type owl:ObjectProperty ;
                   rdfs:domain oh:Cavallo ;
                   rdfs:range oh:AreaGeografica ;
                   owl:propertyChainAxiom ( oh:èScuderizzatoInCittàDi
                                            designpatternpart:isPartOf
                                          ) ;
                   rdfs:comment "Property that expresses the geographic area in terms of the State or Region in which the horse is stabled."@en ,
                                "Proprietà che esprime l'area geografica in termini di Stato o Regione in cui il cavallo è scuderizzato."@it ;
                   rdfs:label "is stabled in"@en ,
                              "è scuderizzato in"@it .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses#èScuderizzatoInCittàDi
oh:èScuderizzatoInCittàDi rdf:type owl:ObjectProperty ,
                                   owl:FunctionalProperty ;
                          rdfs:domain oh:Cavallo ;
                          rdfs:comment "Property that expresses the city in which the horse is stabled"@en ,
                                       "Proprietà che esprime la Città in cui il cavallo è scuderizzato"@it ;
                          rdfs:label "is stabled in the City of"@en ,
                                     "è scuderizzato in Città di"@it .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses#altezzaGarrese
oh:altezzaGarrese rdf:type owl:DatatypeProperty ;
                  rdfs:domain oh:Cavallo ;
                  rdfs:range xsd:decimal ;
                  schema:image <https://www.kramer.it/$WS/kraemer-pferdesport/websale8_shop-kraemer-pferdesport/benutzer/templates/ws-customer/Serviceseiten/images/content/blankets_ita.jpg> ;
                  rdfs:comment "L'altezza di un animale quadrupede si misura al garrese che è il punto più alto del dorso e si trova tra il collo e il tronco nella zona di incontro tra collo e scapole"@it ,
                               "The height of a quadruped animal is measured at the withers which is the highest point of the back and is located between the neck and the trunk in the area where the neck and shoulder blades meet"@en ;
                  rdfs:label "Height at the withers [cm]"@en ,
                             "altezza al garrese [cm]" .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses#altezzaMediaGarrese
oh:altezzaMediaGarrese rdf:type owl:DatatypeProperty ;
                       rdfs:domain oh:RazzaCavallo ;
                       rdfs:range xsd:decimal ;
                       rdfs:comment "Average height at the withers of a horse breed"@en ,
                                    "Valore medio dell'altezza al garrese di una razza di cavallo"@it ;
                       rdfs:label "average height at the withers [cm]"@en ,
                                  "altezza media al garrese [cm]" .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses#eta
oh:eta rdf:type owl:DatatypeProperty ,
                owl:FunctionalProperty ;
       rdfs:domain oh:Cavallo ;
       rdfs:range xsd:integer ;
       rdfs:comment "age of horse for sale"@en ,
                    "età del cavallo in vendita"@it ;
       rdfs:label "age"@en ,
                  "età"@it .


