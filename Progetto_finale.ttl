                                                                   rdf:rest [ rdf:type rdf:List ;
                                                                              rdf:first "falbo" ;
                                                                              rdf:rest [ rdf:type rdf:List ;
                                                                                         rdf:first "grigio" ;
                                                                                         rdf:rest [ rdf:type rdf:List ;
                                                                                                    rdf:first "isabella" ;
                                                                                                    rdf:rest [ rdf:type rdf:List ;
                                                                                                               rdf:first "mantello coniugato" ;
                                                                                                               rdf:rest [ rdf:type rdf:List ;
                                                                                                                          rdf:first "morello" ;
                                                                                                                          rdf:rest [ rdf:type rdf:List ;
                                                                                                                                     rdf:first "palomino" ;
                                                                                                                                     rdf:rest [ rdf:type rdf:List ;
                                                                                                                                                rdf:first "pezzato" ;
                                                                                                                                                rdf:rest [ rdf:type rdf:List ;
                                                                                                                                                           rdf:first "roano" ;
                                                                                                                                                           rdf:rest [ rdf:type rdf:List ;
                                                                                                                                                                      rdf:first "sauro" ;
                                                                                                                                                                      rdf:rest [ rdf:type rdf:List ;
                                                                                                                                                                                 rdf:first "sorcino" ;
                                                                                                                                                                                 rdf:rest [ rdf:type rdf:List ;
                                                                                                                                                                                            rdf:first "ubero" ;
                                                                                                                                                                                            rdf:rest rdf:nil
                                                                                                                                                                                          ]
                                                                                                                                                                               ]
                                                                                                                                                                    ]
                                                                                                                                                         ]
                                                                                                                                              ]
                                                                                                                                   ]
                                                                                                                        ]
                                                                                                             ]
                                                                                                  ]
                                                                                       ]
                                                                            ]
                                                                 ]
                                                      ]
                                           ]
                               ] ;
                    rdfs:comment "Insieme dei colori del mantello ammessi per una razza di cavallo"@it ,
                                 "Set of coat colors allowed for a horse breed"@en ;
                    rdfs:label "admittet coat colors"@en ,
                               "ha colori mantello ammessi"@it .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses#haSesso
oh:haSesso rdf:type owl:DatatypeProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain oh:Cavallo ;
           rdfs:range [ rdf:type rdfs:Datatype ;
                        owl:oneOf [ rdf:type rdf:List ;
                                    rdf:first "castrone" ;
                                    rdf:rest [ rdf:type rdf:List ;
                                               rdf:first "femmina" ;
                                               rdf:rest [ rdf:type rdf:List ;
                                                          rdf:first "maschio" ;
                                                          rdf:rest rdf:nil
                                                        ]
                                             ]
                                  ]
                      ] ;
           rdfs:comment "Horse sex for sale"@en ,
                        "Sesso del cavallo in vendita" ;
           rdfs:label "ha sesso"@it ,
                      "has sex"@en .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses#nomeCavallo
oh:nomeCavallo rdf:type owl:DatatypeProperty ;
               rdfs:domain oh:Cavallo ;
               rdfs:range xsd:string ;
               rdfs:comment "Horse name for sale"@en ,
                            "Nome del cavallo in vendita"@it ;
               rdfs:label "Horse name"@en ,
                          "Nome del cavallo"@it .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses#pesoMedio
oh:pesoMedio rdf:type owl:DatatypeProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain oh:RazzaCavallo ;
             rdfs:range xsd:integer ;
             rdfs:comment "Average mass value of a specific horse breed"@en ,
                          "Valore medio della massa di una specifica razza di cavallo"@it ;
             rdfs:label "medium weight [Kg]"@en ,
                        "peso medio [Kg]"@it .


#################################################################
#    Classes
#################################################################

###  http://dbpedia.org/ontology/HorseRider
dbo:HorseRider rdf:type owl:Class ;
               owl:equivalentClass oh:Cavaliere ;
               rdfs:subClassOf oh:Persona .


###  http://dbpedia.org/ontology/horse
dbo:horse rdf:type owl:Class ;
          owl:equivalentClass oh:Cavallo .


###  http://dbpedia.org/ontology/horseRidingDiscipline
dbo:horseRidingDiscipline rdf:type owl:Class ;
                          owl:equivalentClass oh:Sella ;
                          rdfs:subClassOf oh:Impiego .


###  http://schema.org/Place
schema:Place rdf:type owl:Class ;
             owl:equivalentClass oh:AreaGeografica .


###  http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection
<http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection> rdf:type owl:Class ;
                                                                               owl:equivalentClass oh:CatalogoAutorizzazioniAMontareFise .


###  http://www.ontologydesignpatterns.org/cp/owl/list.owl#List
<http://www.ontologydesignpatterns.org/cp/owl/list.owl#List> rdf:type owl:Class .


###  http://www.ontologydesignpatterns.org/cp/owl/list.owl#ListItem
<http://www.ontologydesignpatterns.org/cp/owl/list.owl#ListItem> rdf:type owl:Class .


###  http://www.ontologydesignpatterns.org/cp/owl/price.owl#Currency
designpatternprice:Currency rdf:type owl:Class ;
                            rdfs:subClassOf oh:Cavallo .


###  http://www.ontologydesignpatterns.org/cp/owl/price.owl#Price
designpatternprice:Price rdf:type owl:Class .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses#AltaScuola
oh:AltaScuola rdf:type owl:Class ;
              rdfs:subClassOf oh:SportEquestre .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses#AreaGeografica
oh:AreaGeografica rdf:type owl:Class ;
                  rdfs:comment "Area spaziale univocamente identificato da coordinate geografiche di latitudine e longitudine"@it ,
                               "Spatial area uniquely identified by geographical coordinates of latitude and longitude"@en ;
                  rdfs:label "Area geografica"@it ,
                             "Geographical area"@en .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses#CatalogoAutorizzazioniAMontareFise
oh:CatalogoAutorizzazioniAMontareFise rdf:type owl:Class ;
