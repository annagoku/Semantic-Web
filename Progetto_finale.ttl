#################################################################
#    Object Properties
#################################################################

###  http://www.ontologydesignpatterns.org/cp/owl/partof.owl#hasPart
<http://www.ontologydesignpatterns.org/cp/owl/partof.owl#hasPart> rdfs:domain ontoHorse:AreaGeografica ;
                                                                  rdfs:range ontoHorse:AreaGeografica .


###  http://www.ontologydesignpatterns.org/cp/owl/partof.owl#isPartOf
<http://www.ontologydesignpatterns.org/cp/owl/partof.owl#isPartOf> rdfs:domain ontoHorse:AreaGeografica ;
                                                                   rdfs:range ontoHorse:AreaGeografica .


###  http://www.ontologydesignpatterns.org/cp/owl/price.owl#hasPrice
<http://www.ontologydesignpatterns.org/cp/owl/price.owl#hasPrice> rdfs:domain ontoHorse:Cavallo .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses#haIndole
:haIndole rdf:type owl:ObjectProperty ,
                   owl:FunctionalProperty ;
          rdfs:domain ontoHorse:RazzaCavallo ;
          rdfs:range :Indole ;
          rdfs:comment "Property that expresses the temperament of a horse breed"@en ,
                       "Proprietà che esprime il temperamento di una razza di cavallo"@it ;
          rdfs:label "ha indole"@it ,
                     "has temperament"@en .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses#haNazionalità
:haNazionalità rdf:type owl:ObjectProperty ;
               rdfs:domain ontoHorse:RazzaCavallo ;
               rdfs:range ontoHorse:AreaGeografica ;
               owl:propertyChainAxiom ( ontoHorse:haRegioneDiOrigine
                                        <http://www.ontologydesignpatterns.org/cp/owl/partof.owl#isPartOf>
                                      ) ;
               rdfs:comment "Property that expresses the nation of origin of a horse breed"@en ,
                            "Proprietà che esprime la nazionalità di una razza di cavallo"@it ;
               rdfs:label "has nationality"@en ,
                          "ha nazionalità" .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses#puòEssereImpiegataIn
:puòEssereImpiegataIn rdf:type owl:ObjectProperty ;
                      rdfs:domain ontoHorse:RazzaCavallo ;
                      rdfs:range ontoHorse:Impiego ;
                      rdfs:comment "Property that expresses the possible uses of a breed in work or sport"@en ,
                                   "Proprietà che esprime i possibili impieghi di una razza nel lavoro o nello sport"@it ;
                      rdfs:label "can be used in"@en ,
                                 "può essere impiegata in"@it .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses/adattoAlConseguimentoDi
ontoHorse:adattoAlConseguimentoDi rdf:type owl:ObjectProperty ;
                                  rdfs:domain ontoHorse:Cavallo ;
                                  rdfs:range ontoHorse:Patente ;
                                  owl:propertyChainAxiom ( ontoHorse:gareggiaNelleCategorieDiSalto
                                                           ontoHorse:permetteIlConseguimentoDi
                                                         ) ;
                                  rdfs:comment "Property that expresses the relationship between the horse's sporting performance and the riding licences it allows to achieve"@en ,
                                               "Proprietà che esprime la relazione tra le prestazioni sportive del cavallo e le autorizzazioni a montare che permette di conseguire"@it ;
                                  rdfs:label "suitable for achieving"@en ,
                                             "adatto al conseguimento di" .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses/consegue
ontoHorse:consegue rdf:type owl:ObjectProperty ;
                   owl:inverseOf ontoHorse:vieneConseguitoDa ;
                   rdfs:domain ontoHorse:Cavaliere ;
                   rdfs:range ontoHorse:Patente ;
                   rdfs:comment "Property that expresses the relationship between the rider's experience and the riding licences he can achieve"@en ,
                                "Proprietà che esprime la relazione tra l'esperienza del cavaliere e l'<utorizzazioni a montare che può conseguire"@it ;
                   rdfs:label "achieve"@en ,
                              "consegue"@it .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses/gareggiaNelleCategorieDiSalto
ontoHorse:gareggiaNelleCategorieDiSalto rdf:type owl:ObjectProperty ;
                                        rdfs:domain ontoHorse:Cavallo ;
                                        rdfs:range ontoHorse:CategorieDiSalto ;
                                        rdfs:comment "Property that expresses the categories of  jumping competitions in which the horse usually participates."@en ,
                                                     "Proprietà che esprime le categorie di gare di salto ad ostacoli alle quali il cavallo di solito partecipa."@it ;
                                        rdfs:label "gareggia nelle categorie di salto ostacoli"@it ,
                                                   "take part to jumping categories"@en .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses/haRazza
ontoHorse:haRazza rdf:type owl:ObjectProperty ,
                           owl:FunctionalProperty ;
                  rdfs:domain ontoHorse:Cavallo ;
                  rdfs:range ontoHorse:RazzaCavallo ;
                  rdfs:comment "Property that expresses the breed of a horse"@en ,
                               "Proprietà che esprime la razza di un cavallo"@it ;
                  rdfs:label "ha razza"@it ,
                             "has breed"@en .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses/haRegioneDiOrigine
ontoHorse:haRegioneDiOrigine rdf:type owl:ObjectProperty ;
                             rdfs:domain ontoHorse:RazzaCavallo ;
                             rdfs:range ontoHorse:AreaGeografica ;
                             rdfs:comment "Property that expresses the territory of origin of a horse breed"@en ,
                                          "Proprietà che esprime il territorio di origine di una razza di cavallo"@it ;
                             rdfs:label "ha Regione di origine"@it ,
                                        "has region of origin"@en .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses/permetteIlConseguimentoDi
ontoHorse:permetteIlConseguimentoDi rdf:type owl:ObjectProperty ;
                                    rdfs:domain ontoHorse:CategorieDiSalto ;
                                    rdfs:range ontoHorse:Patente ;
                                    rdfs:comment "Property that expresses the relationship between the jumping categories in which a rider competes and the riding licences he can obtain. In particular, the C80 allows the obtaining of the patent; the C115 allows the achievement of the 1st degree and the C135 the 2nd degree,"@en ,
                                                 "Proprietà che esprime la relazione tra le categorie di salto ad ostacoli in cui un cavaliera gareggia e le autorizzazioni a montare che può conseguire. In particolare la C80 permette il conseguimento del brevetto; la C115 permette il conseguimento del 1° grado e la C135 del 2° grado"@it ;
                                    rdfs:label "allows the achievement of"@en ,
                                               "permette il conseguiimento di"@it .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses/vieneConseguitoDa
ontoHorse:vieneConseguitoDa rdf:type owl:ObjectProperty ;
                            rdfs:domain ontoHorse:Patente ;
                            rdfs:range ontoHorse:Cavaliere ;
                            rdfs:comment "Inverse property of \"achieve\""@en ,
                                         "Proprietà inversa di \"consegue\""@it ;
                            rdfs:label "is achieved by"@en ,
                                       "viene conseguito da"@it .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses/vieneUtilizzatoPer
ontoHorse:vieneUtilizzatoPer rdf:type owl:ObjectProperty ;
                             rdfs:domain ontoHorse:Cavallo ;
                             rdfs:range ontoHorse:Impiego ;
                             rdfs:comment "Property that expresses the use made of a horse in work or sporting activities."@en ,
                                          "Proprietà che esprime l'utilizzo che viene fatto di un cavallo in attività lavorative o sportive."@it ;
                             rdfs:label "is used for"@en ,
                                        "viene utilizzato per"@it .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses/èAdattoPer
ontoHorse:èAdattoPer rdf:type owl:ObjectProperty ;
                     rdfs:domain ontoHorse:Cavallo ;
                     rdfs:range ontoHorse:Cavaliere ;
                     owl:propertyChainAxiom ( ontoHorse:adattoAlConseguimentoDi
                                              ontoHorse:vieneConseguitoDa
                                            ) ;
                     rdfs:comment "Property that expresses the suitability of a horse for the sporting abilities of a rider. Generally horses that are used in very high jumping categories are not suitable for inexperienced riders."@en ,
                                  "Proprietà che esprime l'adeguatezza di un cavallo alle capacità sportive di un cavaliere. Generalmente cavalli che vengono utilizzati in categorie di salto molto alte non sono idonei a cavalieri poco esperti."@it ;
                     rdfs:label "is suitable for"@en ,
                                "è adatto per"@it .


