#################################################################
#    Object Properties
#################################################################

###  http://www.ontologydesignpatterns.org/cp/owl/partof.owl#isPartOf
<http://www.ontologydesignpatterns.org/cp/owl/partof.owl#isPartOf> rdfs:domain :schema:Place ;
                                                                   rdfs:range :schema:Place .


###  http://www.ontologydesignpatterns.org/cp/owl/price.owl#hasPrice
<http://www.ontologydesignpatterns.org/cp/owl/price.owl#hasPrice> rdfs:domain :Cavallo .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses#haIndole
ontoHorses:haIndole rdf:type owl:ObjectProperty ,
                             owl:FunctionalProperty ;
                    rdfs:domain :dbo:HorseRace ;
                    rdfs:range ontoHorses:Indole .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses#haNazionalità
ontoHorses:haNazionalità rdf:type owl:ObjectProperty ;
                         rdfs:domain :dbo:HorseRace ;
                         rdfs:range :schema:Place ;
                         owl:propertyChainAxiom ( :haRegioneDiOrigine
                                                  <http://www.ontologydesignpatterns.org/cp/owl/partof.owl#isPartOf>
                                                ) .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses#puòEssereImpiegataIn
ontoHorses:puòEssereImpiegataIn rdf:type owl:ObjectProperty ;
                                rdfs:domain :dbo:HorseRace ;
                                rdfs:range :Impiego .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses/Impiega
:Impiega rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses/adattoAlConseguimentoDi
:adattoAlConseguimentoDi rdf:type owl:ObjectProperty ;
                         rdfs:domain :Cavallo ;
                         rdfs:range :Patente ;
                         owl:propertyChainAxiom ( :gareggiaNelleCategorieDiSalto
                                                  :permetteIlConseguimentoDi
                                                ) .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses/consegue
:consegue rdf:type owl:ObjectProperty ;
          owl:inverseOf :vieneConseguitoDa .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses/gareggiaNelleCategorieDiSalto
:gareggiaNelleCategorieDiSalto rdf:type owl:ObjectProperty ;
                               rdfs:domain :Cavallo ;
                               rdfs:range :CategorieDiSalto .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses/haMadre
:haMadre rdf:type owl:ObjectProperty ,
                  owl:FunctionalProperty .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses/haPadre
:haPadre rdf:type owl:ObjectProperty ,
                  owl:FunctionalProperty .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses/haRazza
:haRazza rdf:type owl:ObjectProperty ,
                  owl:FunctionalProperty ;
         rdfs:domain :Cavallo ;
         rdfs:range :dbo:HorseRace .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses/haRegioneDiOrigine
:haRegioneDiOrigine rdf:type owl:ObjectProperty ;
                    rdfs:domain :dbo:HorseRace ;
                    rdfs:range :schema:Place .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses/permetteIlConseguimentoDi
:permetteIlConseguimentoDi rdf:type owl:ObjectProperty ;
                           rdfs:domain :CategorieDiSalto ;
                           rdfs:range :Patente .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses/vieneConseguitoDa
:vieneConseguitoDa rdf:type owl:ObjectProperty .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses/vieneUtilizzatoPer
:vieneUtilizzatoPer rdf:type owl:ObjectProperty ;
                    rdfs:domain :Cavallo ;
                    rdfs:range :Impiego .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses/èAdattoPer
:èAdattoPer rdf:type owl:ObjectProperty ;
            rdfs:domain :Cavallo ;
            owl:propertyChainAxiom ( :adattoAlConseguimentoDi
                                     :vieneConseguitoDa
                                   ) .


