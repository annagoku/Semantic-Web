              rdfs:range xsd:nonNegativeInteger .


#################################################################
#    Classes
#################################################################

###  http://dbpedia.org/ontology/HorseRider
<http://dbpedia.org/ontology/HorseRider> rdf:type owl:Class ;
                                         owl:equivalentClass [ rdf:type owl:Class ;
                                                               owl:oneOf ( :CavaliereEsperto
                                                                           :CavaliereIntermedio
                                                                           :CavaliereMoltoEsperto
                                                                           :CavalierePrincipiante
                                                                         )
                                                             ] ;
                                         rdfs:subClassOf <http://xmlns.com/foaf/0.1/Person> .


###  http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection
<http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection> owl:equivalentClass :CatalogoAutorizzazioniAMontareFise .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses#AltaScuola
ontoHorses:AltaScuola rdf:type owl:Class ;
                      rdfs:subClassOf :SportEquestre .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses#CavalloDaLavoro
ontoHorses:CavalloDaLavoro rdf:type owl:Class ;
                           owl:equivalentClass [ rdf:type owl:Restriction ;
                                                 owl:onProperty :vieneUtilizzatoPer ;
                                                 owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                 owl:onClass :Lavoro
                                               ] ;
                           rdfs:subClassOf :Cavallo ;
                           rdfs:comment "Cavallo impiegato nel lavoro agricolo o nel traino carrozze e/o carichi"@it ,
                                        "Horse used in agricultural work or in pulling carriages and/or loads"@en ;
                           rdfs:label "Cavallo da lavoro"@it ,
                                      "Work horse"@en .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses#CavalloDaTiroAgricolo
ontoHorses:CavalloDaTiroAgricolo rdf:type owl:Class ;
                                 owl:equivalentClass [ rdf:type owl:Restriction ;
                                                       owl:onProperty :vieneUtilizzatoPer ;
                                                       owl:hasValue ontoHorses:TiroAgricolo
                                                     ] ;
                                 rdfs:subClassOf ontoHorses:CavalloDaLavoro ;
                                 rdfs:comment "Cavallo utilizzato nei lavori agricoli"@it ,
                                              "Horse used in agricultural work"@en ;
                                 rdfs:label "Agricultural wor horse"@en ,
                                            "Cavallo da tiro agricolo"@it .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses#CavalloDaTrainoCarrozze
ontoHorses:CavalloDaTrainoCarrozze rdf:type owl:Class ;
                                   owl:equivalentClass [ rdf:type owl:Restriction ;
                                                         owl:onProperty :vieneUtilizzatoPer ;
                                                         owl:hasValue ontoHorses:TrainoCarrozze
                                                       ] ;
                                   rdfs:subClassOf ontoHorses:CavalloDaLavoro ;
                                   rdfs:comment "Cavallo utilizzato per il traino di carrozze merci o passeggeri"@it ,
                                                "Horse used for pulling freight or passenger carriages"@en ;
                                   rdfs:label "Cavallo da traino carrozze"@it ,
                                              "Horse for pulling carriages"@en .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses#CrossCountry
ontoHorses:CrossCountry rdf:type owl:Class ;
                        rdfs:subClassOf :SportEquestre .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses#Dressage
ontoHorses:Dressage rdf:type owl:Class ;
                    rdfs:subClassOf :SportEquestre .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses#Endurance
ontoHorses:Endurance rdf:type owl:Class ;
                     rdfs:subClassOf :SportEquestre .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses#Equiturismo
ontoHorses:Equiturismo rdf:type owl:Class ;
                       rdfs:subClassOf :Sella .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses#Eventing
ontoHorses:Eventing rdf:type owl:Class ;
                    rdfs:subClassOf :SportEquestre .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses#Indole
ontoHorses:Indole rdf:type owl:Class ;
                  owl:equivalentClass [ rdf:type owl:Class ;
                                        owl:oneOf ( ontoHorses:sangueCaldo
                                                    ontoHorses:sangueFreddo
                                                    ontoHorses:sangueMisto
                                                  )
                                      ] ;
                  rdfs:comment "L'indole o temperamento è uno dei classici parametri di classificazione dei cavalli"@it ,
                               "The nature or temperament is one of the classic classification parameters of horses"@en ;
                  rdfs:label "Indole"@it ,
                             "Nature"@en .


