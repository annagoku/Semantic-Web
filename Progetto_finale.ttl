###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses/categorieDiDressage
:categorieDiDressage rdf:type owl:DatatypeProperty ;
                     rdfs:domain :Cavallo ;
                     rdfs:range [ rdf:type rdfs:Datatype ;
                                  owl:oneOf [ rdf:type rdf:List ;
                                              rdf:first "D" ;
                                              rdf:rest [ rdf:type rdf:List ;
                                                         rdf:first "E" ;
                                                         rdf:rest [ rdf:type rdf:List ;
                                                                    rdf:first "F" ;
                                                                    rdf:rest [ rdf:type rdf:List ;
                                                                               rdf:first "M" ;
                                                                               rdf:rest rdf:nil
                                                                             ]
                                                                  ]
                                                       ]
                                            ]
                                ] .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses/età
:età rdf:type owl:DatatypeProperty ;
