                                               owl:unionOf ( [ rdf:type owl:Restriction ;
                                                               owl:onProperty :vieneUtilizzatoPer ;
                                                               owl:someValuesFrom :DaTiroAgricolo
                                                             ]
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty :impiegatoNelLavoroAgricolo ;
                                                               owl:hasValue "true"^^xsd:boolean
                                                             ]
                                                           )
                                             ] ;
                         rdfs:subClassOf :CavalloDaLavoro .


