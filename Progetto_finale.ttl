                            rdfs:range xsd:string ;
                            rdfs:comment "Province in which the horse for sale is stabled"@en ,
                                         "Provincia in cui il cavallo in vendita è scuderizzato"@it ;
                            rdfs:label "province of horse stable"@en ,
                                       "provincia di scuderizzazione"@it .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses#regioneDiScuderizzazione
:regioneDiScuderizzazione rdf:type owl:DatatypeProperty ,
                                   owl:FunctionalProperty ;
                          rdfs:domain ontoHorse:Cavallo ;
                          rdfs:range xsd:string ;
                          rdfs:comment "Region in which the horse for sale is stabled"@en ,
                                       "Regione in cui il cavallo in vendita è scuderizzato"@it ;
                          rdfs:label "region of horse stable"@en ,
                                     "regione di scuderizzazione"@it .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses/altezzaAlGarrese_cm
ontoHorse:altezzaAlGarrese_cm rdf:type owl:DatatypeProperty ,
                                       owl:FunctionalProperty ;
                              rdfs:domain ontoHorse:Cavallo ;
                              rdfs:range xsd:nonNegativeInteger ;
                              <http://schema.org/image> <https://www.kramer.it/$WS/kraemer-pferdesport/websale8_shop-kraemer-pferdesport/benutzer/templates/ws-customer/Serviceseiten/images/content/blankets_ita.jpg> ;
                              rdfs:comment "L'altezza di un animale quadrupede si misura al garrese che è il punto più alto del dorso e si trova tra il collo e il tronco nella zona di incontro tra collo e scapole"@it ,
                                           "The height of a quadruped animal is measured at the withers which is the highest point of the back and is located between the neck and the trunk in the area where the neck and shoulder blades meet"@en ;
                              rdfs:label "altezza al garrese [cm]"@it ,
                                         "height at the withers [cm]"@en .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses/altezzaMaxDiSalto_cm
ontoHorse:altezzaMaxDiSalto_cm rdf:type owl:DatatypeProperty ,
                                        owl:FunctionalProperty ;
                               rdfs:domain ontoHorse:Cavallo ;
                               rdfs:range xsd:nonNegativeInteger ;
                               rdfs:comment "Altezza massima di salto ostacoli registrata in una competizione ufficiale FISE"@it ,
                                            "Maximum jumping height recorded in an official FISE competition"@en ;
                               rdfs:label "Maximum jumping height [cm]"@en ,
                                          "altezza massima di salto ostacoli [cm]"@it .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses/altezzaMediaAlGarrese_cm
ontoHorse:altezzaMediaAlGarrese_cm rdf:type owl:DatatypeProperty ,
                                            owl:FunctionalProperty ;
                                   rdfs:domain ontoHorse:RazzaCavallo ;
                                   rdfs:range xsd:nonNegativeInteger ;
                                   rdfs:comment "Average height at the withers of a horse breed"@en ,
                                                "Valore medio dell'altezza al garrese di una razza di cavallo"@it ;
                                   rdfs:label "Average height at the withers  [cm]"@en ,
                                              "altezza media al garrese [cm]"@it .


###  http://www.semanticweb.org/annag/ontologies/2024/1/ontoHorses/categorieDiDressage
ontoHorse:categorieDiDressage rdf:type owl:DatatypeProperty ;
                              rdfs:domain ontoHorse:Cavallo ;
                              rdfs:range [ rdf:type rdfs:Datatype ;
                                           owl:oneOf [ rdf:type rdf:List ;
                                                       rdf:first "D" ;
                                                       rdf:rest [ rdf:type rdf:List ;
                                                                  rdf:first "E" ;
                                                                  rdf:rest [ rdf:type rdf:List ;
                                                                             rdf:first "F" ;
                                                                             rdf:rest [ rdf:type rdf:List ;
                                                                                        rdf:first "M" ;
                                                                                        rdf:rest rdf:nil
                                                                                      ]
                                                                           ]
                                                                ]
                                                     ]
                                         ] ;
                              rdfs:comment "A Dressage competition is defined by a category that indicates its difficulty. In particular: E (elementary), F (easy), M (medium), D (difficult)"@en ,
                                           "Una competizione di Dressage è definita da una categoria che ne indica la difficolta. In particolare: E (elementare), F (facile), M (media), D (difficile)"@it ;
                              rdfs:label "Dressage competition categories"@en ,
                                         "categorie di Dressage"@it .


