                                           rdf:rest [ rdf:type rdf:List ;
                                                      rdf:first "baio" ;
                                                      rdf:rest [ rdf:type rdf:List ;
                                                                 rdf:first "crema" ;
                                                                 rdf:rest [ rdf:type rdf:List ;
                                                                            rdf:first "falbo" ;
                                                                            rdf:rest [ rdf:type rdf:List ;
                                                                                       rdf:first "grigio" ;
                                                                                       rdf:rest [ rdf:type rdf:List ;
                                                                                                  rdf:first "isabella" ;
                                                                                                  rdf:rest [ rdf:type rdf:List ;
                                                                                                             rdf:first "mantello coniugato" ;
                                                                                                             rdf:rest [ rdf:type rdf:List ;
                                                                                                                        rdf:first "morello" ;
                                                                                                                        rdf:rest [ rdf:type rdf:List ;
                                                                                                                                   rdf:first "palomino" ;
                                                                                                                                   rdf:rest [ rdf:type rdf:List ;
                                                                                                                                              rdf:first "pezzato" ;
                                                                                                                                              rdf:rest [ rdf:type rdf:List ;
                                                                                                                                                         rdf:first "roano" ;
                                                                                                                                                         rdf:rest [ rdf:type rdf:List ;
                                                                                                                                                                    rdf:first "sauro" ;
                                                                                                                                                                    rdf:rest [ rdf:type rdf:List ;
