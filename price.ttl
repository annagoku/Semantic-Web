@prefix : <http://www.ontologydesignpatterns.org/cp/owl/price.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix price: <http://www.ontologydesignpatterns.org/cp/owl/price.owl#> .
@base <http://www.ontologydesignpatterns.org/cp/owl/price.owl#> .

<http://www.ontologydesignpatterns.org/cp/owl/price.owl> rdf:type owl:Ontology ;
                                                          owl:imports <http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl> ;
                                                          rdfs:comment "The price content ontology design pattern. This CP represents the price of an entity in a certain currency." ;
                                                          owl:versionInfo "1.1 added rdfs:isDefinedBy for all named entities" ,
                                                                          "Created by Valentina Presutti" .

#################################################################
#    Annotation properties
#################################################################

###  http://www.w3.org/2000/01/rdf-schema#comment
rdfs:comment rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#isDefinedBy
rdfs:isDefinedBy rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2000/01/rdf-schema#label
rdfs:label rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#versionInfo
owl:versionInfo rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.ontologydesignpatterns.org/cp/owl/price.owl#hasCurrency
price:hasCurrency rdf:type owl:ObjectProperty ;
                  rdfs:domain price:Price ;
                  rdfs:range price:Currency ;
                  rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/price.owl> ;
                  rdfs:label "has currency"@en .


###  http://www.ontologydesignpatterns.org/cp/owl/price.owl#hasPrice
price:hasPrice rdf:type owl:ObjectProperty ;
               owl:inverseOf price:isPriceOf ;
               rdfs:range price:Price ;
               rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/price.owl> ;
               rdfs:label "has price"@en .


###  http://www.ontologydesignpatterns.org/cp/owl/price.owl#isPriceOf
price:isPriceOf rdf:type owl:ObjectProperty ;
                rdfs:domain price:Price ;
                rdfs:range owl:Thing ;
                rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/price.owl> ;
                rdfs:label "is price of"@en .


#################################################################
#    Data properties
#################################################################

###  http://www.ontologydesignpatterns.org/cp/owl/price.owl#hasValue
price:hasValue rdf:type owl:DatatypeProperty ;
               rdfs:domain price:Price ;
               rdfs:range xsd:float ;
               rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/price.owl> ;
               rdfs:label "has value"@en .


#################################################################
#    Classes
#################################################################

###  http://www.ontologydesignpatterns.org/cp/owl/price.owl#Currency
price:Currency rdf:type owl:Class ;
               owl:disjointWith price:Price ;
               rdfs:comment "A system of money in general use in a particular country." ;
               rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/price.owl> ;
               rdfs:label "Currency"@en .


###  http://www.ontologydesignpatterns.org/cp/owl/price.owl#Price
price:Price rdf:type owl:Class ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty price:hasCurrency ;
                              owl:cardinality "1"^^xsd:nonNegativeInteger
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty price:hasValue ;
                              owl:minCardinality "1"^^xsd:nonNegativeInteger
                            ] ;
            rdfs:comment "The amount of money expected, required, or given in payment for something." ;
            rdfs:isDefinedBy <http://www.ontologydesignpatterns.org/cp/owl/price.owl> ;
            rdfs:label "Price"@en .


###  http://www.w3.org/2002/07/owl#Thing
owl:Thing rdf:type owl:Class .


